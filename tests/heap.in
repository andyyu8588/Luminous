class Object {
	public id;
	public constructor(id) {
		this.id = id;
	}
	public printId() {
		print("Object ID is: " + this.id);
	}
}

function idHash(object) {
	return object.id;
}

objectHeap = Heap(idHash);

objectHeap.add(Object(1));
objectHeap.add(Object(17));
objectHeap.add(Object(11));
objectHeap.add(Object(0));
objectHeap.add(Object(8));

print(objectHeap.size());
objectHeap.min().printId();
objectHeap.deleteMin().printId();
objectHeap.deleteMin().printId();
objectHeap.deleteMin().printId();
objectHeap.deleteMin().printId();
objectHeap.min().printId();
objectHeap.deleteMin().printId();