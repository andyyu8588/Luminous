class Test {
    public x;
    constructor(x) {
        this.x = x;
    }

    increment() {
        this.x += 1;
        print(this.x);
    }

    decrement() {
        this.x -= 1;
        print(this.x);
    }

    double() {
        this.x *= 2;
        print(this.x);
    }

    negate() {
        this.x /= -1;
        print(this.x);
    }
}

test = Test(1);
test.increment();
test.increment();
test.double();
test.decrement();
test.negate();

