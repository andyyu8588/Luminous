class Test {
    public x;
    public constructor(x) {
        this.x = x;
    }

    public increment() {
        this.x += 1;
        print(this.x);
    }

    public decrement() {
        this.x -= 1;
        print(this.x);
    }

    public double() {
        this.x *= 2;
        print(this.x);
    }

    public negate() {
        this.x /= -1;
        print(this.x);
    }
}

test = Test(1);
test.increment();
test.increment();
test.double();
test.decrement();
test.negate();

