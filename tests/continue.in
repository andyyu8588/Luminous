for(i from 0 to 10 by 1) {
    if(i > 2 and i < 7) continue;
    print(i);
}

cnt = 0;
while(cnt < 10) {
    if(cnt > 2 and cnt < 7) {
        cnt += 1;
        continue;
    }
    print(cnt);
    cnt += 1;
}

for(i from 0 to 10 by 1) {
    print("J is " + i);
    if(i % 2 equals 0) continue;
    for(j from i to 10 by 1) {
        if(j % 2 equals 0) {
            continue;
        }
        print("Number is odd");
        count = j;
        while(count < 10) {
            if(count % 2 equals 1) {
                count += 1;
                continue;
            }
            print("Number is even");
            count += 1;
        }
    }
}