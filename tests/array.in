class Object1 {
    public x;
    public constructor() {
        this.x = "pogg";
    }
    public printPogg() {
        print(this.x);
    }
}

class Object2 inherits Object1 {
    public x;
    public constructor() {
        this.x = "Not pogg";
    }
    public printPogg() {
        print(this.x);
    }
}

class Container {
    public arr;
    public constructor() {
        this.arr = [Object1(), Object2()];
    }
    public printContainer() {
        print(this.arr);
        for(i from 0 to 2 by 1) {
            this.arr[i].printPogg();
        }
    }
}

container = Container();
container.printContainer();
globalNum = 3.1414;
globalStr = "this is a string";

globalArr = [container, globalNum, globalStr];
print(globalArr);

