// For compiler/VM testing purpose

function collatz(a) {
    print(a);
    if(a equals 1) {
        return;
    }
    else if(a % 2 equals 0) {
        collatz(a / 2);
    }
    else {
        collatz(3 * a + 1);
    }
}
print(collatz);
collatz(15);
collatz(50);
collatz(123);

